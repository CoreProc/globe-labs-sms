{
  "name": "coreproc/laravel-notification-channel-globe-labs-sms",
  "description": "This package makes it easy to send notifications using Globe Labs SMS with Laravel 5.3, 5.4, and 5.5",
  "homepage": "https://github.com/coreproc/globe-labs-sms-notification",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Bautista",
      "email": "chris.bautista@coreproc.ph",
      "homepage": "https://coreproc.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/notifications": "^5.3|^5.4|^5.5|^5.6",
    "illuminate/support": "^5.3|^5.4|^5.5|^5.6",
    "guzzlehttp/guzzle": "^6.2",
    "coreproc/msisdn-ph": "^1.1"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.5",
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "Coreproc\\GlobeLabsSms\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Coreproc\\GlobeLabsSms\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Coreproc\\GlobeLabsSms\\GlobeLabsSmsServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
